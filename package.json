{
  "name": "fastify-mongo-memory",
  "version": "1.1.0",
  "description": "Fastify mongo in memory plugin",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext js",
    "test": "eslint ./ && tap test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nbalduzzi/fastify-mongo-memory.git"
  },
  "keywords": [
    "Fastify",
    "MongoDB",
    "Test"
  ],
  "author": "Nicolas Balduzzi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nbalduzzi/fastify-mongo-memory/issues"
  },
  "homepage": "https://github.com/nbalduzzi/fastify-mongo-memory#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.{js}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "fastify-plugin": "^3.0.0",
    "mongo-in-memory": "0.0.5",
    "mongodb": "^3.6.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint": "^7.19.0",
    "fastify": "^3.11.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "tap": "^14.11.0"
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
